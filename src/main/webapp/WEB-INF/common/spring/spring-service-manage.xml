<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

	<context:annotation-config />

	<bean id="dataProcessSpringInitation" class="org.distributedScheduler.DataProcessSpringInitation"></bean>

	<bean id="dataSourceFactory"
		class="org.distributedScheduler.biz.task.DataSourceFactory"
		init-method="start" destroy-method="shutdown"></bean>

	<bean id="distributedLock"
		class="org.distributedScheduler.biz.lock.impl.ZooKeeperDistributedLock">
	</bean>

	<!-- <bean id="actionExecuterFactory" class="com.taobao.dq.dataProcess.action.ActionExecuterFactory" 
		init-method = "init" destroy-method="shutdown"></bean> -->

</beans>

